{
    "name": "ComposerConnector",
    "version": "1.0.0",
    "title": "Composer Connector",
    "type": "flogo:connector",
    "author": "TIBCO Software Inc.",
    "display": {
        "description": "Composer Connector",
        "category": "SmartContract",
        "visible": true,
        "smallIcon": "composer.png",
        "largeIcon": "composer.png"
    },
    "ref": "github.com/TIBCOSoftware/dovetail-contrib/model/SmartContract/connector/composer",
    "keyfield": "name",
    "settings": [{
            "name": "name",
            "type": "string",
            "required": true,
            "display": {
                "name": "Connector Name",
                "visible": true
            }
        },
        {
            "name": "mode",
            "type": "boolean",
            "required": true,
            "display": {
                "name": "Use exsiting Business Network Archive (.bna) file",
                "visible": true
            },
            "value": true
        },
        {
            "name": "modelFile",
            "type": "string",
            "required": false,
            "display": {
                "name": "Select model file",
                "visible": true ,
                "type": "fileselector", 
                "fileExtensions": [".bna"]
            }
        },
        {
            "name": "enterModel",
            "type": "complex_object",
            "required": false,
            "display": {
                "name": "Enterfo composer model for quick testing",
                "visible": false ,
                "type": "texteditor",
                "syntax": "text"
            }
        },
        {
            "name": "assets",
            "type": "array",
            "required":false,
            "display":{
                "visible":false
            }
        },
        {
            "name": "transactions",
            "type": "array",
            "required":false,
            "display":{
                "visible":false
            }
        },
        {
            "name": "schemas",
            "type": "object",
            "required":false,
            "display":{
                "visible":false
            }
        },
        {
            "name": "events",
            "type": "array",
            "required":false,
            "display":{
                "visible":false
            }
        },
        {
            "name": "participants",
            "type": "array",
            "required":false,
            "display":{
                "visible":false
            }
        },
        {
            "name": "concepts",
            "type": "array",
            "required":false,
            "display":{
                "visible":false
            }
        }
    ],
    "actions": [ 
        {
            "name": "Save Model"
        }
    ]
}