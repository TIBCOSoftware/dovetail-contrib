{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "info": {
        "title": "marble_private",
        "version": "1.1.0"
    },
    "contracts": {
        "marble_private": {
            "name": "marble_private",
            "transactions": [
                {
                    "name": "initMarble",
                    "description": "create a new marble, store into chaincode state",
                    "operation": "invoke",
                    "transient": {
                        "type": "object",
                        "properties": {
                            "marble": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "color": {
                                        "type": "string"
                                    },
                                    "size": {
                                        "type": "number"
                                    },
                                    "owner": {
                                        "type": "string"
                                    },
                                    "price": {
                                        "type": "number"
                                    }
                                }
                            }
                        }
                    },
                    "returns": {
                        "$ref": "#/components/schemas/marble_private_0"
                    }
                },
                {
                    "name": "readMarble",
                    "description": "retrieve a marble state",
                    "operation": "query",
                    "parameters": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            }
                        }
                    },
                    "returns": {
                        "$ref": "#/components/schemas/marble_private_0"
                    }
                },
                {
                    "name": "readMarblePrivateDetails",
                    "description": "read a marble private details from chaincode state",
                    "operation": "query",
                    "parameters": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            }
                        }
                    },
                    "returns": {
                        "type": "object",
                        "properties": {
                            "docType": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            },
                            "price": {
                                "type": "number"
                            }
                        }
                    }
                },
                {
                    "name": "delete",
                    "description": "remove a marble key/value pair from state",
                    "operation": "invoke",
                    "transient": {
                        "type": "object",
                        "properties": {
                            "marble_delete": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "returns": {
                        "$ref": "#/components/schemas/marble_private_0"
                    }
                },
                {
                    "name": "transferMarble",
                    "description": "transfer a marble by setting a new owner name on the marble",
                    "operation": "invoke",
                    "transient": {
                        "type": "object",
                        "properties": {
                            "marble_owner": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "owner": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "returns": {
                        "$ref": "#/components/schemas/marble_private_0"
                    }
                },
                {
                    "name": "getMarblesByRange",
                    "description": "performs a range query based on the start and end keys provided",
                    "operation": "query",
                    "parameters": {
                        "type": "object",
                        "properties": {
                            "startKey": {
                                "type": "string"
                            },
                            "endKey": {
                                "type": "string"
                            }
                        }
                    },
                    "returns": {
                        "$ref": "#/components/schemas/marble_private_array0"
                    }
                },
                {
                    "name": "queryMarblesByOwner",
                    "description": "queries for marbles based on a passed-in owner",
                    "operation": "query",
                    "parameters": {
                        "type": "object",
                        "properties": {
                            "owner": {
                                "type": "string"
                            }
                        }
                    },
                    "returns": {
                        "$ref": "#/components/schemas/marble_private_array0"
                    }
                }
            ]
        }
    },
    "components": {
        "schemas": {
            "marble_private_0": {
                "$id": "marble_private_0",
                "type": "object",
                "properties": {
                    "docType": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "color": {
                        "type": "string"
                    },
                    "size": {
                        "type": "number"
                    },
                    "owner": {
                        "type": "string"
                    }
                }
            },
            "marble_private_array0": {
                "$id": "marble_private_array0",
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "key": {
                            "type": "string"
                        },
                        "value": {
                            "type": "object",
                            "properties": {
                                "docType": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "color": {
                                    "type": "string"
                                },
                                "size": {
                                    "type": "number"
                                },
                                "owner": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}